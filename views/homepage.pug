doctype html 
html(lang="en")
    head 
        title !{title}
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body
        h1 Custom Object Table 
        a(href="/update-cobj") Add to this table 
        table 
            tr
                th Name 
                th Brand 
                th Model 
                th Color 
            each object in data
                tr  
                    td #{object.properties.name} 
                    td #{object.properties.brand}
                    td #{object.properties.model}
                    td #{object.properties.color}
